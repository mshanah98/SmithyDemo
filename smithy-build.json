{
  "version": "1.0",
  "projections": {
    "typescript": {
      "plugins": {
        "typescript-codegen": {
          "service": "demo#DemoService",
          "package": "@demo/demo-service",
          "packageVersion": "1.0.0"
        }
      }
    },
    "apigateway": {
      "plugins": {
        "openapi": {
          "service": "demo#DemoService",
          "protocol": "aws.protocols#restJson1"
        }
      }
    }
  }
}